# Tell versions [3.59,3.63) of GNU make to not export all variables.
# Otherwise a system limit (for SysV at least) may be exceeded.
.NOEXPORT:

#>+ 2
docs-am:

#>+ 5
force-reedit:
		cd $(top_srcdir) && $(AUTOMAKE) --gnu gemrb/Makefile
	cd $(top_srcdir) && perl admin/am_edit gemrb/Makefile.in


#>+ 11
gemrb.all_cpp.cpp: $(srcdir)/Makefile.in $(srcdir)/GemRB.cpp  
	@echo 'creating gemrb.all_cpp.cpp ...'; \
	rm -f gemrb.all_cpp.files gemrb.all_cpp.final; \
	echo "#define KDE_USE_FINAL 1" >> gemrb.all_cpp.final; \
	for file in GemRB.cpp ; do \
	  echo "#include \"$$file\"" >> gemrb.all_cpp.files; \
	  test ! -f $(srcdir)/$$file || egrep '^#pragma +implementation' $(srcdir)/$$file >> gemrb.all_cpp.final; \
	done; \
	cat gemrb.all_cpp.final gemrb.all_cpp.files  > gemrb.all_cpp.cpp; \
	rm -f gemrb.all_cpp.final gemrb.all_cpp.files

#>+ 3
clean-final:
	-rm -f gemrb.all_cpp.cpp

#>+ 2
final:
	$(MAKE) all-am
#>+ 2
final-install:
	$(MAKE) install-am
#>+ 2
no-final:
	$(MAKE) all-am
#>+ 2
no-final-install:
	$(MAKE) install-am
#>+ 3
cvs-clean:
	$(MAKE) -f $(top_srcdir)/admin/Makefile.common cvs-clean

#>+ 3
kde-rpo-clean:
